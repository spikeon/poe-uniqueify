{
  "name": "poe-uniqueify",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.45",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-redux": "^7.1.32",
    "i18next": "^21.10.0",
    "overwolf-hooks": "^3.0.6",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.6",
    "react-redux": "^8.1.3",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-remote-server": "node remotedev-server",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "pack-overwolf": "node create-production-overwolf-build",
    "dist-overwolf": "cd build && tar -acf ../dist.v$npm_package_version.zip * && cd -",
    "create-opk": "cd build/app && tar -acf ../../dist-opk.v$npm_package_version.zip * && cd - && node create-opk",
    "build:overwolf": "npm run build && npm run pack-overwolf && npm run dist-overwolf && npm run create-opk"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@overwolf/types": "^3.24.0",
    "@types/remote-redux-devtools": "^0.5.8",
    "remote-redux-devtools": "^0.5.16",
    "remotedev-server": "^0.3.1"
  },
  "homepage": "."
}
